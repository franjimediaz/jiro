// Migraciones npx prisma migrate dev --name init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Obra {
  id          Int      @id @default(autoincrement())
  nombre      String
  direccion   String
  fechaInicio DateTime
  fechaFin    DateTime
  estado      String
  createdAt   DateTime @default(now())
}
model Cliente {
  id       Int     @id @default(autoincrement())
  nombre   String
  email    String
  telefono String
}
